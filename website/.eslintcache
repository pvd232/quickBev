n  },\n  formButton: {\n    height: \"fit-content\",\n  },\n}));\nexport default function Chart(props) {\n  const theme = useTheme();\n  const [business, setBusiness] = useState(\"all\");\n\n  console.log(\"props.data\", props.data);\n  // Generate Sales Data\n  var orders;\n  orders = props.data.orders.map((orderObject) => {\n    return new Order(orderObject);\n  });\n  for (var i in orders) {\n    console.log(\"order\", orders[i]);\n  }\n  const handleBusiness = (event) => {\n    const value = event.target.value;\n    console.log(\"value\", value);\n    setBusiness(value);\n  };\n  const createData = (order) => {\n    console.log(\"order\", order);\n    const time = order.dateTime;\n    console.log(\"time\", time);\n    const amount = order.cost;\n    return { time, amount };\n  };\n\n  const classes = useStyles();\n\n  const dataFormatted = orders.map((order) => createData(order));\n  console.log(\"dataFormatted\", dataFormatted);\n  return (\n    <>\n      <Title>Today</Title>\n      <FormControl variant=\"outlined\" className={classes.formControl}>\n        <InputLabel id=\"demo-simple-select-outlined-label\">Business</InputLabel>\n        <Select\n          labelId=\"demo-simple-select-outlined-label\"\n          id=\"demo-simple-select-outlined\"\n          value={business}\n          className={classes.formButton}\n          onChange={handleBusiness}\n          label=\"Business\"\n          displayEmpty\n        >\n          <MenuItem value={\"all\"}>All Businesses</MenuItem>\n          <MenuItem value={10}>Ten</MenuItem>\n          <MenuItem value={20}>Twenty</MenuItem>\n          <MenuItem value={30}>Thirty</MenuItem>\n        </Select>\n      </FormControl>\n      <ResponsiveContainer>\n        <LineChart\n          data={dataFormatted}\n          margin={{\n            top: 16,\n            right: 16,\n            bottom: 0,\n            left: 24,\n          }}\n        >\n          <XAxis dataKey=\"time\" stroke={theme.palette.text.secondary} dy={10} />\n          <YAxis stroke={theme.palette.text.secondary} dx={-5}>\n            <Label\n              angle={270}\n              position=\"left\"\n              style={{ textAnchor: \"middle\", fill: theme.palette.text.primary }}\n            >\n              Sales ($)\n            </Label>\n          </YAxis>\n          <Line\n            type=\"monotone\"\n            dataKey=\"amount\"\n            stroke={theme.palette.primary.main}\n            dot={false}\n          />\n        </LineChart>\n      </ResponsiveContainer>\n    </>\n  );\n}\n","/Users/machina/Developer/quickbev/website/src/pages/dashboard/listItems.js",[],"/Users/machina/Developer/quickbev/website/src/pages/dashboard/Orders.js",[],"/Users/machina/Developer/quickbev/website/src/pages/dashboard/Deposits.js",[],["98","99"],"/Users/machina/Developer/quickbev/website/src/pages/dashboard/Title.js",[],"/Users/machina/Developer/quickbev/website/src/pages/PayoutSetup.js",["100","101"],"import React, { useState, useEffect } from \"react\";\nimport Button from \"react-bootstrap/Button\";\nimport API from \"../helpers/Api.js\";\nimport bankIcon from \"../static/icon-bank.svg\";\nimport { Business, setLocalStorage } from \"../Models.js\";\n\nconst PayoutSetup = (props) => {\n  const [redirect, setRedirect] = useState(null);\n  var redirectUrl = null;\n  const getRedirectInfo = async () => {\n    return API.makeRequest(\n      \"GET\",\n      `http://127.0.0.1:5000/create-stripe-account`\n    );\n  };\n  const onSubmit = async (event, businessStripeId) => {\n    event.preventDefault();\n    if (localStorage.getItem(\"business\")) {\n      const currentBusiness = new Business(\n        localStorage.getItem(\"business\"),\n        true\n      );\n      currentBusiness.stripeId = businessStripeId;\n      setLocalStorage(\"business\", currentBusiness);\n\n      const dataObject = { business: currentBusiness };\n      let result = await API.makeRequest(\n        \"POST\",\n        \"http://127.0.0.1:5000/signup-redirect\",\n        dataObject,\n        false\n      );\n      console.log(\"Success:\", result);\n      return true;\n    } else {\n      console.log(\"No user business found\");\n    }\n  };\n  const handleConnect = async () => {\n    let [responseBody, response] = await getRedirectInfo();\n    const businessStripeId = response.headers.get(\"stripe_id\");\n    let url = responseBody.url;\n    if (url && businessStripeId) {\n      redirectUrl = url;\n      return businessStripeId;\n    }\n  };\n  useEffect(() => {\n    //had to do this because memory leak due to component not unmounting properly\n    let mount = true;\n    if (mount && redirect) {\n      window.location.assign(redirect);\n    }\n\n    return () => (mount = false);\n  }, [redirect]);\n\n  if (props.callback) {\n    return (\n      <>\n        <div className=\"text-center box\">\n          <img src={bankIcon} alt=\"\" className=\"icon\" />\n          <h5 style={{ marginTop: \"5%\", marginBottom: \"5%\" }}>\n            Stripe account onboarding incomplete\n          </h5>\n          <p>\n            Please click the button below to be redirected to Stripe to complete\n            the onboarding proces\n          </p>\n\n          <Button\n            className=\"btn btn-primary text-center\"\n            onClick={(event) => {\n              handleConnect().then((businessStripeId) =>\n                onSubmit(event, businessStripeId).then((result) => {\n                  // setRedirect(redirectUrl);\n                })\n              );\n            }}\n          >\n            Set up payouts\n          </Button>\n        </div>\n      </>\n    );\n  } else {\n    return (\n      <>\n        <div className=\"text-center box\">\n          <div className=\"fs-title\">\n            Input your bank account to recieve payouts\n          </div>\n          <img src={bankIcon} alt=\"\" className=\"icon\" />\n\n          <p>\n            QuickBev partners with Stripe to transfer earnings to your bank\n            account.\n          </p>\n\n          <Button\n            className=\"btn btn-primary text-center\"\n            onClick={(event) => {\n              handleConnect().then((businessStripeId) =>\n                props\n                  .onSubmit(event, businessStripeId, false)\n                  .then((result) => {\n                    // setRedirect(redirectUrl);\n                  })\n              );\n            }}\n          >\n            Set up payouts\n          </Button>\n\n          <p className=\"text-center notice\">\n            You'll be redirected to Stripe to complete the onboarding proces.\n          </p>\n        </div>\n      </>\n    );\n  }\n};\n\nexport default PayoutSetup;\n","/Users/machina/Developer/quickbev/website/src/helpers/Api.js",[],"/Users/machina/Developer/quickbev/website/src/pages/PayoutSetupCallback.js",[],"/Users/machina/Developer/quickbev/website/src/pages/Splash.js",[],"/Users/machina/Developer/quickbev/website/src/pages/dashboard/Customers.js",[],"/Users/machina/Developer/quickbev/website/src/pages/dashboard/HomeSplash.js",[],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":3,"column":10,"nodeType":"108","messageId":"109","endLine":3,"endColumn":18},{"ruleId":"106","severity":1,"message":"110","line":3,"column":20,"nodeType":"108","messageId":"109","endLine":3,"endColumn":28},{"ruleId":"106","severity":1,"message":"111","line":3,"column":30,"nodeType":"108","messageId":"109","endLine":3,"endColumn":45},{"ruleId":"112","severity":1,"message":"113","line":74,"column":24,"nodeType":"114","messageId":"115","endLine":74,"endColumn":35},{"ruleId":"106","severity":1,"message":"116","line":15,"column":8,"nodeType":"108","messageId":"109","endLine":15,"endColumn":22},{"ruleId":"102","replacedBy":"117"},{"ruleId":"104","replacedBy":"118"},{"ruleId":"106","severity":1,"message":"119","line":8,"column":20,"nodeType":"108","messageId":"109","endLine":8,"endColumn